#!/bin/bash

[ -f $(dirname $0)/../lib/em.shlib ] && . $(dirname $0)/../lib/em.shlib || { echo "$(dirname $0)/../lib/em.shlib not found" 1>&2; exit 1; }

function main()
{
    process-common-arguments $@

    for arg in ${args}; do
        echo "> ${arg}"
    done

    echo "debug: ${debug}"

}

main $@