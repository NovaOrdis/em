#!/bin/bash
#
# the entry point in em. This script should be placed in PATH
#

[ -f $(dirname $0)/../em.shlib ] && . $(dirname $0)/../em.shlib || { echo "$(dirname $0)/../em.shlib not found" 1>&2; exit 1; }

function main
{
    local command

    while [ "$1" != "" ]; do
        if [ "$1" = "--debug" ]; then
            debug_off=false
        elif [ "${command}" = "" ]; then
            command=$1
        fi
        shift
    done

    debug "command: ${command}"

}

main $@